
@{
    ViewBag.Title = "Signup";
}
<h1>Signup</h1>
<div class="jumbotron">
    <h3>회원가입을 진행하세요.</h3>
    <form asp-controller="Home" asp-action="Login">
        <p>
            <div>ID</div>
            <input id="Text1" type="text" name="newID" />
        </p>
        <p>
            <div>Password</div>
            <input id="Text2" type="password" name="newPWD" />
        </p>
        <input id="Submit1" type="submit" value="가입하기." />
    </form>
</div>
